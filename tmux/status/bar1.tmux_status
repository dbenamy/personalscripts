# bar1.tmux_status

# everything black on magenta
set -g status-fg black
set -g status-bg magenta

set-option -g status-justify "left"

set -g status-left-length 8
set -g status-left '#(tmux_gitbranch.py)'
set -g status-right '#[fg=white]#(whoami)#[fg=yellow]@#[default]#[fg=cyan bright]#H#[default] #[default][#S] #(tmux_time.py)'

